            <xs:maxLength value="15"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="MACType">
        <xs:restriction base="xs:string">
            <xs:pattern value="(([0-9a-fA-F]{2}[-]){5}([0-9a-fA-F]{2}))"/>
            <xs:length value="17"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="HostType">
        <xs:restriction base="xs:string">
