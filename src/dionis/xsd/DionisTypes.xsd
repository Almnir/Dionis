    <xs:simpleType name="IPType">
        <xs:restriction base="xs:string">
            <xs:pattern value="(0.0.0.0|((25[0-5]|2[0-4]\d|[01]?\d\d?)\.){3}(25[0-5]|2[0-4]\d|[01]?\d\d?))"/>
